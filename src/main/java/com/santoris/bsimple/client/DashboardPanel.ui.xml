<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:com.github.gwtbootstrap.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
		
		.social li {
			display: inline !important;
		}
	</ui:style>
	<g:FlowPanel>
		<b:ResponsiveNavbar position="TOP" scrollspy="true">
			<b:Brand href="#">bsimple</b:Brand>
			<b:NavCollapse>
				<b:Nav ui:field="nav" />
			</b:NavCollapse>
		</b:ResponsiveNavbar>

		<b:FluidContainer ui:field="container">
			<b:FluidRow>
				<b:Column size="9">
					<g:FlowPanel ui:field="pursesContainer" />
				</b:Column>
				<b:Column size="3" styleName="pull-right">
					<g:FlowPanel ui:field="transactionsContainer" />
				</b:Column>
			</b:FluidRow>

<!-- 			<b:Footer> -->
<!-- 			</b:Footer> -->
		</b:FluidContainer>
	</g:FlowPanel>
</ui:UiBinder>